/* webview 兼容*/
[data-weui-theme=dark].page{
    background-color: #111;
    color: hsla(0,0%,100%,.8);
}
[data-weui-theme=dark] .page__desc{
    color: hsla(0,0%,100%,.5);
}
[data-weui-theme=dark] .kind-list__item {
    background-color: #191919;
}
[data-weui-theme=dark] .weui-cells {
    background-color: #191919;
}
[data-weui-theme=dark] .weui-cell {
    color: hsla(0,0%,100%,.8);
}
[data-weui-theme=dark] .weui-footer {
    color: hsla(0,0%,100%,.3);
}
/* cell */
[data-weui-theme=dark] .weui-cell__ft {
    color: hsla(0,0%,100%,.5);
}
[data-weui-theme=dark] .weui-slideview__btn-group_default .weui-slideview__btn {
    background-color: #404040;
}
/* slideview */
[data-weui-theme=dark] .weui-slidecell {
    background-color: #191919;
}
[data-weui-theme=dark] .weui-slideview_icon .weui-slideview__btn {
    background-color: #404040;
}
/* form */
[data-weui-theme=dark] .weui-form {
    background-color: #191919;
}
[data-weui-theme=dark] .weui-form__text-area {
    color: hsla(0,0%,100%,.8);
}
[data-weui-theme=dark] .weui-cells__title {
    color: hsla(0,0%,100%,.5);
}
[data-weui-theme=dark] .weui-cells_checkbox .weui-icon-checked {
    color: hsla(0,0%,100%,.3);
}
/* uploader */
[data-weui-theme=dark] .weui-uploader__info,
[data-weui-theme=dark] .weui-uploader__tips {
    color: hsla(0,0%,100%,.3);
}
[data-weui-theme=dark] .weui-uploader__input-box {
    background-color: #2e2e2e;
}
/* article */
[data-weui-theme=dark] .weui-article {
    color: hsla(0,0%,100%,.8);
}
/* icons */
[data-weui-theme=dark] .weui-grid__label {
    color: hsla(0,0%,100%,.8);
}
/* panel */
[data-weui-theme=dark] .weui-panel {
    background-color: #191919;
}
[data-weui-theme=dark] .weui-panel__hd,
[data-weui-theme=dark] .weui-media-box__title {
    color: hsla(0,0%,100%,.8);
}
[data-weui-theme=dark] .weui-media-box__desc,
[data-weui-theme=dark] .weui-media-box__info {
    color: hsla(0,0%,100%,.3);
}
/* grid */
.weui-grid__label {
    color: hsla(0,0%,100%,.8);
}
/* loadmore */
[data-weui-theme=dark] .weui-loadmore__tips {
    color: hsla(0,0%,100%,.8);
}
[data-weui-theme=dark] .weui-loadmore_line .weui-loadmore__tips {
    background-color: #111;
}
[data-weui-theme=dark] .weui-loadmore_line {
    border-top: 1px solid hsla(0,0%,100%,.5);
}
[data-weui-theme=dark] .weui-loadmore__tips__before {
    background-color: hsla(0,0%,100%,.8);
}
/* preview */
[data-weui-theme=dark] .weui-form-preview {
    background-color: #191919;
}
[data-weui-theme=dark] .weui-form-preview__label {
    color: hsla(0,0%,100%,.5);
}
[data-weui-theme=dark] .weui-form-preview__value_in-hd,
[data-weui-theme=dark] .weui-form-preview__value {
    color: hsla(0,0%,100%,.8);
}
[data-weui-theme=dark] .weui-form-preview__btn_default {
    color: hsla(0,0%,100%,.6);
}
/* dialog */
[data-weui-theme=dark] .weui-dialog {
    background-color: #191919;
}
[data-weui-theme=dark] .weui-dialog__bd {
    color: hsla(0,0%,100%,.5);
}
[data-weui-theme=dark] .weui-dialog__btn_default {
    color: hsla(0,0%,100%,.6);
}
[data-weui-theme=dark] .weui-dialog__btn:active {
    background-color: #202020;
}
/* msg */
[data-weui-theme=dark] .weui-msg {
    background-color: #191919;
}
[data-weui-theme=dark] .weui-msg__title {
    color: #d1d1d1;
}
[data-weui-theme=dark] .weui-msg__desc {
    color: hsla(0,0%,100%,.8);
}
/* half-screen-dialog */
[data-weui-theme=dark] .weui-half-screen-dialog {
    background-color: #191919;
}
[data-weui-theme=dark] .weui-half-screen-dialog__bd, 
[data-weui-theme=dark] .weui-half-screen-dialog__title, 
[data-weui-theme=dark] .weui-half-screen-dialog__desc {
    color: hsla(0,0%,100%,.8);
}
[data-weui-theme=dark] .weui-icon-close-thin {
    color: hsla(0,0%,100%,.8);
    background-image: url("data:image/svg+xml;base64,");
}
[data-weui-theme=dark] .weui-half-screen-dialog__tips {
    color: hsla(0,0%,100%,.3);
}
[data-weui-theme=dark] .weui-half-screen-dialog__subtitle {
    color: hsla(0,0%,100%,.5);
}
[data-weui-theme=dark] .weui-icon-more {
    background-image: url("data:image/svg+xml;base64,")
}
/* actionsheet */
[data-weui-theme=dark] .weui-actionsheet {
    background-color: #1e1e1e;
}
[data-weui-theme=dark] .weui-actionsheet__title {
    color: hsla(0,0%,100%,.5);
    background-color: #191919;
}
[data-weui-theme=dark] .weui-actionsheet__action,
[data-weui-theme=dark] .weui-actionsheet__menu {
    color: hsla(0,0%,100%,.8);
    background-color: #191919;
}
[data-weui-theme=dark] .weui-actionsheet__cell:active {
    background-color: #1e1e1e;
}
/* toptips */







/* navigation-bar */
[data-weui-theme=dark] .weui-navigation-bar {
    color: hsla(0,0%,100%,.8);
}
[data-weui-theme=dark] .weui-navigation-bar__inner .weui-navigation-bar__left .weui-navigation-bar__btn_goback {
    background: url("data:image/svg+xml;base64,")
        no-repeat 50% 50%;
}
[data-weui-theme=dark] .weui-navigation-bar__inner .weui-navigation-bar__left .weui-navigation-bar__btn_home {
    background: url("data:image/svg+xml;base64,")
        no-repeat 50% 50%;
}
/* tabbar */
[data-weui-theme=dark] .weui-tabbar {
    background-color: #1e1e1e;
}
[data-weui-theme=dark] .weui-tabbar__label {
    color: hsla(0,0%,100%,.8);
}
/* search-bar */
[data-weui-theme=dark] .weui-search-bar {
    background-color: #111;
}
[data-weui-theme=dark] .weui-search-bar__form {
    background-color: #191919;
}
[data-weui-theme=dark] .weui-search-bar__label {
    color: hsla(0,0%,100%,.5);
    background-color: #191919;
}
[data-weui-theme=dark] .weui-icon-search {
    color: hsla(0,0%,100%,.5);
    background-image: url(data:image/svg+xml;base64,);
}



